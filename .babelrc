{
  "presets": [
    // transpile ES6, ES7, ES8 to ES5
    "@babel/preset-env",
    // tranpile jsx code to ES5
    "@babel/preset-react"
  ],
  "env": {
    "development": {
      "plugins": [
        ["@babel/plugin-proposal-class-properties"],
        ["emotion", { "sourceMap": true }]
      ]
    }
  }
}
